<h1>New faq</h1>

<% form_for(@faq) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :user %><br />
    <%= f.text_field :user %>
  </p>
  <p>
    <%= f.label :duvida %><br />
    <%= f.text_area :duvida %>
  </p>
